<h3>以下の内容で、送信する。</h3>
<%= form_with(model: @post, url: posts_path, local: true) do |form| %>
  
  <%= form.hidden_field :content %>
 
  <p>本文:<%= @post.content %></p>
  <div class="actions">
  <%= form.submit '投稿画面に戻る', name: 'back' %>
  </div>
   <div class="actions">
   <%= form.submit '投稿する' %>
   </div>  
  
<% end %>
